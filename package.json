{
  "name": "fixmyberlin-frontend",
  "version": "1.0.0",
  "description": "A Mobility Platform for Berlin",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "start": "webpack-dev-server",
    "lint": "eslint .",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu BACKEND=staging jest --verbose --config=jest/jest.config.js",
    "test:watch": "npm run test  -- --watch --verbose false",
    "test:coverage": "npm run test -- --coverage",
    "cypress": "cypress",
    "test:e2e": "cypress run",
    "test:e2e-chrome": "cypress run --browser chrome",
    "test:e2e-dev-server": "start-server-and-test start http://localhost:8080 test:e2e",
    "test:e2e-interactive": "cypress open",
    "generate-kataster-schema": "./src/pages/KatasterKI/scheme/generate.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "extract": "formatjs extract 'src/**/*.{j,t}s*' --out-file src/lang/translations/de.json",
    "compile": "(npm run compile:de; npm run compile:en; npm run compile:es) && npx prettier --write 'src/lang/**/*.json'",
    "compile:de": "formatjs compile src/lang/translations/de.json --ast --out-file src/lang/compiled/de.json",
    "compile:en": "formatjs compile src/lang/translations/en.json --ast --out-file src/lang/compiled/en.json",
    "compile:es": "formatjs compile src/lang/translations/es.json --ast --out-file src/lang/compiled/es.json"
  },
